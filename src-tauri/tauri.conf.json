{
  "$schema": "https://raw.githubusercontent.com/nicbarker/tauri/tauri-v2.0.0-alpha.0/tooling/cli/schema.json",
  "productName": "Second",
  "version": "0.1.0",
  "identifier": "com.second.desktop",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "Second",
        "fullscreen": false,
        "resizable": true,
        "width": 1024,
        "height": 768
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
